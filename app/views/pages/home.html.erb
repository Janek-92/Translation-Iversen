<body class="background">
  <div class="homeHeadContent">
    <div class="navbar navbar-expand-lg navbar-light">
      <div class="navbar-brand">
        <img src="<%= image_path "logo1" %>" alt="Iversen Logo" class="avatar-square">
      </div>
      <div class="d-flex justify-content-end align-items-center m-4">
        <% if user_signed_in? %>
        <!--<%= current_user.email %>-->
          <%= link_to 'SIGN OUT', destroy_user_session_path, method: 'delete', :class => "nav-text" %>
        <% else %>
          <%= link_to 'SIGN UP', new_user_registration_path, :class => "nav-text" %>
          <%= link_to 'LOGIN', new_user_session_path, :class => "nav-text"%>
        <% end %>
      </div>
    </div>
  </div>


  <video playsinline="playsinline" autoplay="autoplay" muted="muted" loop="loop" class="video-intro">
    <source src="<%= image_path "intro.mp4" %>" type="video/mp4">
  </video>


  <div class="footer">
    <div class="footer">
      <a href="#"><i class="fab fa-github"></i></a>
      <a href="https://linkedin.com/in/janek-iversen-10511786"><i class="fab fa-linkedin"></i></a>
    </div>
    <div class="footer-copyright">
      Translation & Interpretation Iversen (c) Copyright 2022
    </div>
  </div>
</body>



